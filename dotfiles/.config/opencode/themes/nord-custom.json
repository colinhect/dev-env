      "light": "glacier"
    },
    "markdownImageText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCodeBlock": {
      "dark": "fg",
      "light": "black"
    },
    "syntaxComment": {
      "dark": "light_gray",
      "light": "light_gray"
    },
    "syntaxKeyword": {
      "dark": "glacier",
      "light": "glacier"
    },
    "syntaxFunction": {
      "dark": "off_blue",
      "light": "off_blue"
    },
    "syntaxVariable": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxString": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "syntaxNumber": {
      "dark": "purple",
      "light": "purple"
    },
    "syntaxType": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxOperator": {
      "dark": "glacier",
      "light": "glacier"
    },
    "syntaxPunctuation": {
      "dark": "fg",
      "light": "black"
    }
