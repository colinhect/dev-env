    "diffHighlightAdded": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "diffHighlightRemoved": {
      "dark": "bright_red",
      "light": "bright_red"
    },
    "diffAddedBg": {
      "dark": "dark_gray",
      "light": "white"
