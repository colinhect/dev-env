{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#121213",
    "fg": "#ECEFF4",
    "black": "#07080A",
    "dark_gray": "#3B4252",
    "gray": "#434C5E",
    "light_gray": "#4C566A",
    "light_gray_bright": "#616E88",
    "red": "#B48EAD",
    "green": "#738eb0",
    "yellow": "#8FBCBB",
    "blue": "#5E81AC",
    "magenta": "#82b97c",
    "cyan": "#8FBCBB",
    "white": "#E5E9F0",
    "darkest_white": "#D8DEE9",
    "teal": "#8FBCBB",
    "off_blue": "#88C0D0",
    "glacier": "#81A1C1",
    "orange": "#D08770",
    "purple": "#B48EAD",
    "bright_black": "#4C566A",
    "bright_red": "#BF616A",
    "bright_green": "#A3BE8C",
    "bright_yellow": "#EBCB8B",
    "bright_blue": "#5E81AC",
    "bright_magenta": "#B48EAD",
    "bright_cyan": "#8FBCBB",
    "bright_white": "#ECEFF4"
  },
  "theme": {
    "primary": {
      "dark": "off_blue",
      "light": "blue"
    },
    "secondary": {
      "dark": "glacier",
      "light": "glacier"
    },
    "accent": {
      "dark": "cyan",
      "light": "cyan"
    },
    "error": {
      "dark": "bright_red",
      "light": "bright_red"
    },
    "warning": {
      "dark": "orange",
      "light": "orange"
    },
    "success": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "info": {
      "dark": "off_blue",
      "light": "blue"
    },
    "text": {
      "dark": "fg",
      "light": "black"
    },
    "textMuted": {
      "dark": "light_gray",
      "light": "dark_gray"
    },
    "background": {
      "dark": "bg",
      "light": "fg"
    },
    "backgroundPanel": {
      "dark": "dark_gray",
      "light": "white"
