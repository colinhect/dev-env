      "dark": "fg",
      "light": "black"
    },
    "markdownHeading": {
      "dark": "off_blue",
      "light": "blue"
    },
    "markdownLink": {
      "dark": "glacier",
      "light": "glacier"
    },
    "markdownLinkText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCode": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "markdownBlockQuote": {
      "dark": "light_gray",
      "light": "light_gray"
    },
    "markdownEmph": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownStrong": {
      "dark": "bright_yellow",
      "light": "bright_yellow"
    },
    "markdownHorizontalRule": {
      "dark": "light_gray",
      "light": "light_gray"
    },
    "markdownListItem": {
      "dark": "off_blue",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "cyan",
      "light": "cyan"
