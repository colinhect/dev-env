    "backgroundElement": {
      "dark": "dark_gray",
      "light": "darkest_white"
    },
    "border": {
      "dark": "gray",
      "light": "light_gray"
    },
    "borderActive": {
      "dark": "light_gray",
      "light": "gray"
    },
    "borderSubtle": {
      "dark": "gray",
      "light": "light_gray"
